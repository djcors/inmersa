<uwsgi>
    <pythonpath>/var/www/inmersa/</pythonpath>
    <processes>2</processes>
    <virtualenv>/var/www/inmersa/virtualenv/</virtualenv>
    <pythonpath>/var/virtualenvs/</pythonpath>
    <env>DJANGO_SETTINGS_MODULE=inmersa.settings</env>
    <module>inmersa.wsgi</module>
    <touch-reload>/var/www/inmersa/inmersa.reload</touch-reload>
    <master/>
    <harakiri>600</harakiri>
    <uid>ubuntu</uid>
    <gid>www-data</gid>
    <post-buffering>8192</post-buffering>
    <plugin>python</plugin>
    <socket>/tmp/uwsgi.sock</socket>
    <chmod-socket>666</chmod-socket>
</uwsgi>